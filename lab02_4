      "cell_type": "code",
      "execution_count": null,
      "id": "824e7c1e",
      "metadata": {
        "id": "824e7c1e",
        "outputId": "4bd67ea3-f42f-4a5d-f3a9-d56d6d50b7bf"
      },
      "outputs": [
        {
          "data": {
            "text/plain": [
              "((672, 6), (169, 6), (672,), (169,))"
            ]
          },
          "execution_count": 13,
          "metadata": {},
          "output_type": "execute_result"
        }
      ],
      "source": [
        "from sklearn.model_selection import train_test_split\n",
        "X = df.drop(columns=['Dangerous'])\n",
        "y = df['Dangerous']\n",
        "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.2, random_state=101)\n",
        "X_train.shape, X_test.shape, y_train.shape, y_test.shape"
      ]
    },
    {
      "cell_type": "markdown",
      "id": "a4e197c7",
      "metadata": {
        "id": "a4e197c7"
      },
      "source": [
        "### Mã hóa biến phân loại và chuẩn hóa"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "id": "c0ae58dc",
      "metadata": {
        "id": "c0ae58dc"
      },
      "outputs": [],
      "source": [
        "from sklearn.preprocessing import LabelEncoder, MinMaxScaler\n",
        "# One-Hot Encoding cho features\n",
        "X_train_le = X_train.copy()\n",
        "X_test_le = X_test.copy()\n",
        "\n",
        "label_encoders = {}\n",
        "for col in X_train.columns:\n",
        "    le = LabelEncoder()\n",
        "    # Fit trên train\n",
        "    X_train_le[col] = le.fit_transform(X_train[col])\n",
        "\n",
        "    # Transform test, xử lý giá trị mới\n",
        "    X_test_le[col] = X_test[col].map(\n",
        "        lambda x: le.transform([x])[0] if x in le.classes_ else -1\n",
        "    )\n",
        "    label_encoders[col] = le\n",
        "\n",
        "# Label Encoding cho y\n",
        "le_y = LabelEncoder()\n",
        "y_train_le = le_y.fit_transform(y_train)\n",
        "y_test_le = le_y.transform(y_test)\n",
        "\n",
        "# Chuẩn hóa dữ liệu\n",
        "scaler = MinMaxScaler()\n",
        "X_train_scaled = scaler.fit_transform(X_train_le)\n",
        "X_test_scaled = scaler.transform(X_test_le)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "id": "388c5736",
