      },
      "outputs": [],
      "source": [
        "from sklearn.preprocessing import LabelEncoder\n",
        "le_gender = LabelEncoder()\n",
        "le_smoking_history = LabelEncoder()\n",
        "X_train['gender_n'] = le_gender.fit_transform(X_train['gender'])\n",
        "X_train['smoking_history_n'] = le_smoking_history.fit_transform(X_train['smoking_history'])\n",
        "X_train = X_train.drop(columns=['gender', 'smoking_history'])\n",
        "# Làm tương tự với tập test\n",
        "X_test['gender_n'] = le_gender.transform(X_test['gender'])\n",
        "X_test['smoking_history_n'] = le_smoking_history.transform(X_test['smoking_history'])\n",
        "X_test = X_test.drop(columns=['gender', 'smoking_history'])"
      ]
    },
