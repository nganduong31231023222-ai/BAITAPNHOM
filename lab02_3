        "train_acc = clf.score(X_train,y_train)\n",
        "val_acc = clf.score(X_test,y_test)\n",
        "print('Training accuracy: {}'.format(train_acc))\n",
        "print('Validation accuracy: {}'.format(val_acc))"
      ]
