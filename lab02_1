            "\n",
            "  df['Embarked'].fillna(embarked_mode, inplace=True)\n"
          ]
        }
      ],
      "source": [
        "# Calculate the median of the 'Age' column and fill the missing values\n",
        "age_median = df['Age'].median()\n",
        "df['Age'].fillna(age_median, inplace=True)\n",
        "\n",
        "# Calculate the mode of the 'Embarked' column and fill the missing values\n",
        "# .mode() can return multiple values if modes are tied, so we take the first one [0]\n",
        "embarked_mode = df['Embarked'].mode()[0]\n",
        "df['Embarked'].fillna(embarked_mode, inplace=True)\n",
        "\n",
        "# Drop the 'Cabin' column due to excessive missing values\n",
        "df.drop('Cabin', axis=1, inplace=True)\n",
        "\n",
        "# Verify that the missing values have been filled and the 'Cabin' column is dropped\n",
        "print(\"Number of missing values after handling:\")\n",
